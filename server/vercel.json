
{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "cron":[{
    "name":"cron",
    "schedule":"* * * * *",
    "command":"npm run cron",
    "output":{
      "background":true
    }
  }]
}